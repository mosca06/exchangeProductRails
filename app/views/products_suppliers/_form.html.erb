<%= link_to "Index", products_suppliers_path %>
<%= form_with model: @products_supplier do |form| %>
  <%= form.label "Product" %>
  <%= form.collection_select :product_id, products, :id, :name, prompt: "Select a product" %>
  <br>

  <%= form.label "Supplier" %>
  <%= form.collection_select :supplier_id, suppliers, :id, :name, prompt: "Select a supplier" %>
  <br>
  
  <%= form.submit %>
<% end %>
